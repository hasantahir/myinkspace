<!--		</page>-->
	</param>
	<effect>
	<object-type>all</object-type>
		<effects-menu>
			<submenu name="FabLab Chemnitz">
				<submenu name="Paper/Cardboard Boxes"/>
			</submenu>
		</effects-menu>
	</effect>
	<script>
		<command location="inx" interpreter="python">OrigamiPatterns/Template.py</command>
	</script>
</inkscape-extension>